{
    "manifest_version": 2,
    "name": "Screenshot clone",
    "description": "tira screenshots da pagina",
    "version": "1.0",
    "permissions": [
      "contextMenus",
      "activeTab"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  